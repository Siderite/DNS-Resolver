{
	"manifest_version" : 2,

	"name" : "Siderite's DNS Resolver",
	"description" : "Resolve DNS addresses based on previously working IPs",
	"version" : "0.0.1",

	"permissions" : [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"storage"
	],
	"browser_action" : {
		"default_icon" : "icon.png"
	},
	"background" : {
		"scripts" : ["background.js"],
		"persistent" : true
	}
}